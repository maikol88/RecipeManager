<%= render 'shared/page_title', title: 'Create a New Cuisine' %>

<%= render 'shared/errors', obj: @style %>

<div class ="row">
  <div class="well col-md-8 col-md-offset-2">
    <%= form_for @style do |f|%>
        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.submit 'Create New Cuisine', class: "btn btn-success button-profile" %>
    <% end %>
  </div>
</div>